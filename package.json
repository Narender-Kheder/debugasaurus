{
  "name": "debugasourus",
  "displayName": "debugasourus",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onLanguage:plaintext"
  ],
  "main": "./extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "debugasourusView",
          "title": "Debugasourus",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "debugasourusView": [
        {
          "id": "debugasourusTree",
          "name": "Debugasourus Panel"
        }
      ]
    },
    "commands": [
      {
        "command": "debugasourus.checkGitErrors",
        "title": "Check Git Errors"
      },
      {
        "command": "debugasourus.startChat",
        "title": "Start Debugasourus Chat"
      },
      {
        "command": "debugasourus.triggerCustomCompletion",
        "title": "Trigger Custom Completion"
      },
      {
        "command": "debugasourus.refactorCode",
        "title": "Refactor Code"
      },
      {
        "command": "debugasourus.optimizeCode",
        "title": "Optimize Code"
      },
      {
        "command": "debugasourus.generateComments",
        "title": "Generate Comments"
      },
      {
        "command": "debugasourus.debugCode",
        "title": "Debug Python Code"
      }
    ],
    "keybindings": [
      {
        "command": "debugasourus.triggerCustomCompletion",
        "key": "cmd+ctrl+a"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test",
    "test2": "jest"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^5.2.0",
    "eslint": "^9.13.0",
    "jest": "^29.7.0",
    "mocha": "^11.1.0"
  },
  "dependencies": {
    "axios": "^1.8.3",
    "dotenv": "^16.4.7",
    "yarn": "^1.22.22"
  },
  "files": [
    "features/**"
  ]
}
