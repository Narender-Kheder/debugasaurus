{
  "name": "debugasourus",
  "displayName": "debugasourus",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*",
    "onLanguage:plaintext"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "debugasourus.checkGitErrors",
        "title": "Check Git Errors"
      },
      {
        "command": "debugasourus.startChat",
        "title": "Start Debugasourus Chat"
      },
      {
        "command": "debugasourus.triggerCustomCompletion",
        "title": "Trigger Custom Completion"
      },
      {
        "command": "debugasourus.refactorCode",
        "title": "Refactor Code"
      },
      {
        "command": "debugasourus.optimizeCode",
        "title": "Optimize Code"
      },
      {
        "command": "debugasourus.generateComments",
        "title": "Generate Comments"
      }
    ],
    "keybindings": [
      {
        "command": "debugasourus.triggerCustomCompletion",
        "key": "cmd+ctrl+a"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "dotenv": "^16.4.7"
  },
  "files": [
    "features/**"
  ]
}
